{
  "name": "nashorn-webpack-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate showing server rendering using Nashorn with webpack, react, redux, router, etc.",
  "main": "server.js",
  "keywords": [ "express", "webpack", "react", "redux", "router", "java", "nashorn", "server-rendering" ],
  "author": "Arran White",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.3",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "stats-webpack-plugin": "^0.2.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0",
    "autoprefixer": "^6.0.3",
    "css-loader": "^0.19.0",
    "style-loader": "^0.12.4",
    "postcss-loader": "^0.6.0"
  }
}
